local filename = "LOS.txt"
local HttpService = game:GetService("HttpService")
local Player = game:GetService('Players').LocalPlayer
local CoreGui = game:GetService('CoreGui')

local settingsTable = {
    autorings = false,
    autorace = false
}

function loadSettings()
    print("Loading settings...")
    if isfile and isfile(filename) then
        settingsTable = HttpService:JSONDecode(readfile(filename))
        print("Settings loaded")
        print("--New Values are--")
        for i, v in pairs(settingsTable) do
            print(i, v)
        end
    end
end

function saveSettings()
    print("Saving user settings...")
    local json = HttpService:JSONEncode(settingsTable)
    writefile(filename, json)
end

loadSettings()

local Player = game:GetService('Players').LocalPlayer
Player.Idled:connect(function()
	game:GetService('VirtualUser'):Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
	wait(1)
	game:GetService('VirtualUser'):Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
end)

local CoreGui = game:GetService('CoreGui')
if CoreGui:FindFirstChild('LegendsOfSpeed') ~= nil then
	CoreGui:FindFirstChild('LegendsOfSpeed'):Destroy()
end
if CoreGui:FindFirstChild('LegendsOfSpeedEggs') ~= nil then
	CoreGui:FindFirstChild('LegendsOfSpeedEggs'):Destroy()
end
local LegendsOfSpeed = Instance.new('ScreenGui', game:GetService('CoreGui'))
LegendsOfSpeed.Name = 'LegendsOfSpeed'
LegendsOfSpeed.ResetOnSpawn = false

local Header = Instance.new('Frame', LegendsOfSpeed)
Header.Name = 'Header'
Header.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
Header.BorderColor3 = Color3.fromRGB(255, 255, 255)
Header.BorderSizePixel = 0
Header.Position = UDim2.new(0, -295, 0, 10)
Header.Size = UDim2.new(0, 250, 0, 40)

local Frame = Instance.new('Frame', Header)
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BorderColor3 = Color3.fromRGB(255, 255, 255)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0, 0, 0, 40)
Frame.Size = UDim2.new(0, 250, 0, 5)
Frame.Name = 'Header/Background'

local Hide = Instance.new('TextButton', Header)
Hide.Name = 'Hide'
Hide.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
Hide.BorderColor3 = Color3.fromRGB(255, 255, 255)
Hide.BorderSizePixel = 0
Hide.Position = UDim2.new(0, 252, 0, 37)
Hide.Size = UDim2.new(0, 35, 0, 35)
Hide.Font = Enum.Font.Highway
Hide.Text = '<'
Hide.TextColor3 = Color3.fromRGB(255, 255, 255)
Hide.TextSize = 28

local HideToggle = false
Hide.MouseButton1Click:connect(function()
	if HideToggle == false then
		CoreGui.LegendsOfSpeed.Header:TweenPosition(UDim2.new(0, -250, 0, 10), 'In', 'Back', 0.5)
		wait(0.5)
		CoreGui.LegendsOfSpeed.Header.Hide.Text = '>'
		CoreGui.LegendsOfSpeed.Header.Close.BackgroundTransparency = 1
		CoreGui.LegendsOfSpeed.Header.Hide.BackgroundTransparency = 1
		HideToggle = true
	else
		if HideToggle == true then
			CoreGui.LegendsOfSpeed.Header:TweenPosition(UDim2.new(0, 5, 0, 10), 'In', 'Back', 0.5)
			wait(0.5)
			CoreGui.LegendsOfSpeed.Header.Hide.Text = '<'
			CoreGui.LegendsOfSpeed.Header.Close.BackgroundTransparency = 0
			CoreGui.LegendsOfSpeed.Header.Hide.BackgroundTransparency = 0
			HideToggle = false
		end
	end
end)

local EggsGUI = Instance.new('TextButton', Header)
EggsGUI.Name = 'EggsGUI'
EggsGUI.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
EggsGUI.BackgroundTransparency = 1
EggsGUI.BorderColor3 = Color3.fromRGB(255, 255, 255)
EggsGUI.BorderSizePixel = 0
EggsGUI.Position = UDim2.new(0, 252, 0, 74)
EggsGUI.Size = UDim2.new(0, 35, 0, 35)
EggsGUI.Font = Enum.Font.Highway
EggsGUI.Text = 'ðŸ¥š'
EggsGUI.TextColor3 = Color3.fromRGB(255, 255, 255)
EggsGUI.TextSize = 28

EggsGUI.MouseButton1Click:connect(function()
	CoreGui.LegendsOfSpeed.Header:TweenPosition(UDim2.new(0, -295, 0, 10), 'In', 'Back', 0.5)
	wait(0.5)
	CoreGui.LegendsOfSpeed.Header.Hide.Text = '<'
	CoreGui.LegendsOfSpeed.Header.Close.BackgroundTransparency = 0
	CoreGui.LegendsOfSpeed.Header.Hide.BackgroundTransparency = 0
	HideToggle = false
	if CoreGui:FindFirstChild('LegendsOfSpeedEggs') ~= nil then
		CoreGui.LegendsOfSpeedEggs.Header:TweenPosition(UDim2.new(0, 5, 0, 10), 'In', 'Back', 0.5)
	end
	if CoreGui:FindFirstChild('LegendsOfSpeedEggs') == nil then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/BoaHacker/ROBLOX/main/Games/Legends-Of-Speed-Eggs', true))()
	end
end)

