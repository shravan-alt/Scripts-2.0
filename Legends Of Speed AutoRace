game:GetService('ReplicatedStorage').raceInProgress.Changed:Connect(function()
    if settingsTable.autorace and game:GetService('ReplicatedStorage').raceInProgress.Value then
        game:GetService('ReplicatedStorage').rEvents.raceEvent:FireServer("joinRace")
    end
end)

game:GetService('ReplicatedStorage').raceStarted.Changed:Connect(function()
    if settingsTable.autorace and game:GetService('ReplicatedStorage').raceStarted.Value then
        for i, v in pairs(game:GetService('Workspace').raceMaps:GetChildren()) do
            local OldFinishPosition = v.finishPart.CFrame
            v.finishPart.CFrame = Player.Character.HumanoidRootPart.CFrame
            wait()
            v.finishPart.CFrame = OldFinishPosition
        end
    end
end)

while true do
    for i, v in pairs(game:GetService('Workspace').orbFolder[Player.currentMap.Value]:GetChildren()) do
        if AutoSpawnersToggle == true then
            if v:FindFirstChildOfClass('MeshPart') then
                v.outerGem.CFrame = Player.Character.HumanoidRootPart.CFrame
            end
            if v:FindFirstChildOfClass('Part') then
                v.outerOrb.CFrame = Player.Character.HumanoidRootPart.CFrame
            end
        end
    end
    for i, v in pairs(game:GetService('Workspace').Hoops:GetChildren()) do
        if AutoRingsToggle == true and settingsTable.autorings then
            local OldHoopPosition = v.CFrame
            v.CFrame = Player.Character.HumanoidRootPart.CFrame
            wait(0.1)  -- Adjust this delay as needed
            v.CFrame = OldHoopPosition
        end
    end
    if AutoRebirthToggle == true then
        game:GetService('ReplicatedStorage').rEvents.rebirthEvent:FireServer("rebirthRequest")
    end
    wait()
end
