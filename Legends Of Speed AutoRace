Close.TextSize = 28

Close.MouseButton1Click:connect(function()
	AutoSpawnersToggle = false
	AutoRingsToggle = false
	AutoRebirthToggle = false
	AutoRaceToggle = false
	HidePopupsToggle = false
	HideToggle = false
	CoreGui.LegendsOfSpeed.Header:TweenPosition(UDim2.new(0, -295, 0, 10), 'In', 'Back', 0.5)
	wait(0.5)
	CoreGui.LegendsOfSpeed:Destroy()
	if CoreGui:FindFirstChild('LegendsOfSpeedEggs') ~= nil then
		CoreGui.LegendsOfSpeedEggs:Destroy()
	end
end)

