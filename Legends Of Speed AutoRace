AutoRings.Position = UDim2.new(0, 125, 0, 0)
AutoRings.Size = UDim2.new(0, 125, 0, 35)
AutoRings.Font = Enum.Font.Highway
AutoRingsToggle = settingsTable.autorings or false
AutoRings.Text = 'Auto Rings:' .. (AutoRingsToggle and 'âœ…' or 'âŽ')
AutoRings.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoRings.TextSize = 16

AutoRings.MouseButton1Click:Connect(function()
    AutoRingsToggle = not AutoRingsToggle
    settingsTable.autorings = AutoRingsToggle
    AutoRings.Text = 'Auto Rings:' .. (AutoRingsToggle and 'âœ…' or 'âŽ')
    saveSettings() -- Call saveSettings() to store the updated setting
end)

